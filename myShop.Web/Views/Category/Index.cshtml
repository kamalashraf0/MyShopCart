@model IEnumerable<Category>

<h3>Categories List </h3>

<table class="table table-hover" width="100%">
        <tr>
            <td>Name</td>
            <td>Description</td>
            <td>Date Created</td>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.CreatedTime</td>
            </tr>
        
        }

       
        
</table>